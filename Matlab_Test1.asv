x = linspace(0, 15);
f1 = x^2+5;
f2 = x*3;
f3 = x/4;
f4 = x^5+2;

subplot(2, 2, 1)
plot(x , f1 , 'g', 'linewidth' ,2)
xlabel('An x Label')
ylabel('A y Label')
subplot(2, 2, 2)
plot(x , f2 , 'r', 'linewidth' ,2)
subplot(2, 2, 3)
plot(x , f3 , 'y', 'linewidth' ,2)
subplot(2, 2, 4)
plot(x , f4 , 'b', 'linewidth' ,2)

